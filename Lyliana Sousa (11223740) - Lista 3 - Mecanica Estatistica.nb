Cell[TextData[{
 "Para calcular o comprimento m\[EAcute]dio do aglomerado fa\[CCedilla]o: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", "L", "\[RightAngleBracket]"}], "=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"L", "=", "0"}], "\[Infinity]"], 
      RowBox[{"L", " ", 
       SubscriptBox["P", "L"]}]}], "=", " ", 
     RowBox[{
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"L", "=", "0"}], "\[Infinity]"], 
       RowBox[{
        SuperscriptBox["L", "2"], 
        FractionBox[
         SuperscriptBox["e", 
          RowBox[{"\[Beta]\[Epsilon]", "(", 
           RowBox[{"3", "-", "L"}], ")"}]], 
         SuperscriptBox[
          RowBox[{"[", 
           RowBox[{"2", 
            RowBox[{"cosh", "(", "\[Beta]\[Epsilon]", ")"}]}], "]"}], 
          RowBox[{"L", "+", "1"}]]]}]}], " ", "=", 
      RowBox[{
       RowBox[{
        FractionBox[
         SuperscriptBox["e", 
          RowBox[{"3", "\[Beta]\[Epsilon]"}]], 
         RowBox[{"2", 
          RowBox[{"cosh", "(", "\[Beta]\[Epsilon]", ")"}]}]], " ", 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"L", "=", "0"}], "\[Infinity]"], 
         FractionBox[
          RowBox[{
           SuperscriptBox["L", "2"], 
           SuperscriptBox["e", 
            RowBox[{"-", "\[Beta]\[Epsilon]L"}]]}], 
          SuperscriptBox[
           RowBox[{"[", 
            RowBox[{"2", 
             RowBox[{"cosh", "(", "\[Beta]\[Epsilon]", ")"}]}], "]"}], 
           "L"]]}]}], " ", "=", 
       RowBox[{
        RowBox[{
         FractionBox[
          SuperscriptBox["e", 
           RowBox[{"3", "\[Beta]\[Epsilon]"}]], 
          RowBox[{
           SuperscriptBox["e", "\[Beta]\[Epsilon]"], "+", 
           SuperscriptBox["e", 
            RowBox[{"-", "\[Beta]\[Epsilon]"}]]}]], " ", 
         RowBox[{
          UnderoverscriptBox["\[Sum]", 
           RowBox[{"L", "=", "0"}], "\[Infinity]"], 
          SuperscriptBox[
           RowBox[{
            SuperscriptBox["L", "2"], "[", 
            FractionBox[
             SuperscriptBox["e", 
              RowBox[{"-", "\[Beta]\[Epsilon]"}]], 
             RowBox[{
              SuperscriptBox["e", "\[Beta]\[Epsilon]"], "+", 
              SuperscriptBox["e", 
               RowBox[{"-", "\[Beta]\[Epsilon]"}]]}]], "]"}], "L"]}]}], "=", 
        RowBox[{
         RowBox[{
          FractionBox["1", 
           RowBox[{
            SuperscriptBox["e", 
             RowBox[{
              RowBox[{"-", "2"}], "\[Beta]\[Epsilon]"}]], "+", 
            SuperscriptBox["e", 
             RowBox[{
              RowBox[{"-", "4"}], "\[Beta]\[Epsilon]"}]]}]], " ", 
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"L", "=", "0"}], "\[Infinity]"], 
           SuperscriptBox[
            RowBox[{
             SuperscriptBox["L", "2"], "[", 
             FractionBox["1", 
              RowBox[{
               SuperscriptBox["e", 
                RowBox[{"2", "\[Beta]\[Epsilon]"}]], "+", "1"}]], "]"}], 
            "L"]}]}], "=", " ", 
         RowBox[{
          RowBox[{
           FractionBox["1", 
            RowBox[{
             SuperscriptBox["e", 
              RowBox[{
               RowBox[{"-", "2"}], "\[Beta]\[Epsilon]"}]], "+", 
             SuperscriptBox["e", 
              RowBox[{
               RowBox[{"-", "4"}], "\[Beta]\[Epsilon]"}]]}]], " ", 
           SuperscriptBox["e", 
            RowBox[{
             RowBox[{"-", "6"}], " ", "\[Beta]\[Epsilon]"}]], " ", 
           RowBox[{"(", 
            RowBox[{"1", "+", 
             SuperscriptBox["e", 
              RowBox[{"2", " ", "\[Beta]\[Epsilon]"}]]}], ")"}], " ", 
           RowBox[{"(", 
            RowBox[{"2", "+", 
             SuperscriptBox["e", 
              RowBox[{"2", " ", "\[Beta]\[Epsilon]"}]]}], ")"}]}], " ", "=", 
          RowBox[{
           FractionBox[
            RowBox[{
             RowBox[{"(", 
              RowBox[{"1", "+", 
               SuperscriptBox["e", 
                RowBox[{"2", " ", "\[Beta]\[Epsilon]"}]]}], ")"}], " ", 
             RowBox[{"(", 
              RowBox[{"2", "+", 
               SuperscriptBox["e", 
                RowBox[{"2", " ", "\[Beta]\[Epsilon]"}]]}], ")"}]}], 
            RowBox[{
             SuperscriptBox["e", 
              RowBox[{"2", "\[Beta]\[Epsilon]"}]], "+", 
             SuperscriptBox["e", 
              RowBox[{"4", "\[Beta]\[Epsilon]"}]]}]], "  ", "=", " ", 
           RowBox[{
            FractionBox[
             RowBox[{
              RowBox[{"(", 
               RowBox[{"1", "+", 
                SuperscriptBox["e", 
                 RowBox[{"2", " ", "\[Beta]\[Epsilon]"}]]}], ")"}], " ", 
              RowBox[{"(", 
               RowBox[{"2", "+", 
                SuperscriptBox["e", 
                 RowBox[{"2", " ", "\[Beta]\[Epsilon]"}]]}], ")"}]}], 
             RowBox[{
              SuperscriptBox["e", 
               RowBox[{"2", "\[Beta]\[Epsilon]"}]], "(", 
              RowBox[{"1", " ", "+", " ", 
               SuperscriptBox["e", 
                RowBox[{"2", "\[Beta]\[Epsilon]"}]]}], ")"}]], " ", "=", "  ", 
            RowBox[{
             FractionBox[
              RowBox[{"(", 
               RowBox[{"2", "+", 
                SuperscriptBox["e", 
                 RowBox[{"2", " ", "\[Beta]\[Epsilon]"}]]}], ")"}], 
              SuperscriptBox["e", 
               RowBox[{"2", "\[Beta]\[Epsilon]"}]]], "=", " ", 
             RowBox[{
              RowBox[{"2", 
               SuperscriptBox["e", 
                RowBox[{
                 RowBox[{"-", "2"}], "\[Beta]\[Epsilon]"}]]}], " ", "+", "1", 
              " "}]}]}]}]}]}]}]}]}]}]}], TraditionalForm]],
