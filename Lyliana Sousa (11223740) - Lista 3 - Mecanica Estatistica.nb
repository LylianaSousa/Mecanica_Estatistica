       RowBox[{"\[CapitalOmega]", "(", "E", ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["e", 
          RowBox[{"ln", 
           SqrtBox[
            FractionBox[
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                FractionBox["E", "\[Epsilon]"], " ", "+", " ", 
