Cell[TextData[{
 "O valor m\[EAcute]dio do momento de quadrupolo do sistema, sendo ele ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Q", "=", " ", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "N"], 
      SubsuperscriptBox["\[Sigma]", "i", "2"]}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]],ExpressionUUID->"12a354ae-b518-4f56-bc49-b83552a96449"],
 ", \[EAcute] dado por: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", "Q", "\[RightAngleBracket]"}], " ", "=", 
    " ", 
    RowBox[{
     UnderscriptBox["\[Sum]", "j"], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"i", "=", "1"}], "N"], 
        SubsuperscriptBox["\[Sigma]", "i", "2"]}], ")"}], 
      SubscriptBox["P", "j"], " "}]}]}], TraditionalForm]],ExpressionUUID->
  "74dd7e36-7fe6-4941-9517-954d25b90bd9"],
 ", onde ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "j"], "=", " ", 
    RowBox[{
     FractionBox[
      SuperscriptBox["e", 
       RowBox[{"-", 
        SubscriptBox["\[Beta]E", "j"]}]], "Z"], "=", " ", 
     FractionBox[
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", "\[Beta]"}], 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"i", "=", "1"}], "N"], 
         RowBox[{"(", 
          RowBox[{
           SubsuperscriptBox["D\[Sigma]", "i", "2"], " ", "-", " ", 
           SubscriptBox["\[Mu]h\[Sigma]", "i"]}], ")"}]}]}], ")"}], 
      RowBox[{" ", 
       SuperscriptBox[
        RowBox[{"[", 
         RowBox[{"1", " ", "+", " ", 
          RowBox[{"2", 
           SuperscriptBox["e", 
            RowBox[{"-", "\[Beta]D", " "}]], 
           RowBox[{"cosh", "(", "\[Beta]\[Mu]h", ")"}]}]}], " ", "]"}], "N"], 
       "  "}]]}]}], TraditionalForm]],ExpressionUUID->
  "22875958-275c-48f6-9873-bbd6aa2c3002"],
 ". Com isso chego na seguinte express\[ATilde]o para o valor m\[EAcute]dio \
do quadrupolo:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", "Q", "\[RightAngleBracket]"}], " ", "=", 
    " ", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          SubscriptBox["\[Sigma]", "i"], "}"}], "=", "0"}], ",", " ", 
        RowBox[{"\[PlusMinus]", "1"}]}], "N"], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"i", "=", "1"}], "N"], 
         SubsuperscriptBox["\[Sigma]", "i", "2"]}], ")"}], 
       FractionBox[
        RowBox[{"exp", "(", 
         RowBox[{
          RowBox[{"-", "\[Beta]"}], 
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"i", "=", "1"}], "N"], 
           RowBox[{"(", 
            RowBox[{
             SubsuperscriptBox["D\[Sigma]", "i", "2"], " ", "-", " ", 
             SubscriptBox["\[Mu]h\[Sigma]", "i"]}], ")"}]}]}], ")"}], 
        RowBox[{" ", 
         SuperscriptBox[
          RowBox[{"[", 
           RowBox[{"1", " ", "+", " ", 
            RowBox[{"2", 
             SuperscriptBox["e", 
              RowBox[{"-", "\[Beta]D", " "}]], 
             RowBox[{"cosh", "(", "\[Beta]\[Mu]h", ")"}]}]}], " ", "]"}], 
          "N"], "  "}]]}]}], " ", "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", "Z"], " ", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            SubscriptBox["\[Sigma]", "i"], "}"}], "=", "0"}], ",", " ", 
          RowBox[{"\[PlusMinus]", "1"}]}], "N"], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"-", 
           FractionBox["1", "D"]}], ")"}], 
         RowBox[{
          FractionBox["\[PartialD]", 
           RowBox[{"\[PartialD]", "D"}]], 
          RowBox[{"exp", "(", 
           RowBox[{
            RowBox[{"-", "\[Beta]"}], 
            RowBox[{
             UnderoverscriptBox["\[Sum]", 
              RowBox[{"i", "=", "1"}], "N"], 
             RowBox[{"(", 
              RowBox[{
               SubsuperscriptBox["D\[Sigma]", "i", "2"], " ", "-", " ", 
               SubscriptBox["\[Mu]h\[Sigma]", "i"]}], ")"}]}]}], 
           ")"}]}]}]}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"-", " ", 
         FractionBox["1", "Z\[Beta]"]}], " ", 
        RowBox[{
         FractionBox["\[PartialD]", 
          RowBox[{"\[PartialD]", "D"}]], " ", 
         RowBox[{
          UnderoverscriptBox["\[Sum]", 
           RowBox[{
            RowBox[{
             RowBox[{"{", 
              SubscriptBox["\[Sigma]", "i"], "}"}], "=", "0"}], ",", " ", 
            RowBox[{"\[PlusMinus]", "1"}]}], "N"], 
          RowBox[{"exp", "(", 
           RowBox[{
            RowBox[{"-", "\[Beta]"}], 
            RowBox[{
             UnderoverscriptBox["\[Sum]", 
              RowBox[{"i", "=", "1"}], "N"], 
             RowBox[{"(", 
              RowBox[{
               SubsuperscriptBox["D\[Sigma]", "i", "2"], " ", "-", " ", 
               SubscriptBox["\[Mu]h\[Sigma]", "i"]}], ")"}]}]}], ")"}]}]}]}], 
       "  ", "=", " ", 
       RowBox[{
        RowBox[{"-", " ", 
         FractionBox["1", "Z\[Beta]"]}], " ", 
        FractionBox[
         RowBox[{"\[PartialD]", "Z"}], 
         RowBox[{"\[PartialD]", "D"}]]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"bc8d004f-1f5a-4c47-bec8-903131466ba6"],
 "\nSendo, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", "Z"}], 
     RowBox[{"\[PartialD]", "D"}]], "=", " ", 
    RowBox[{
     RowBox[{"N", "  ", 
      SuperscriptBox[
       RowBox[{"[", 
        RowBox[{"1", " ", "+", " ", 
         RowBox[{"2", 
          SuperscriptBox["e", 
           RowBox[{"-", "\[Beta]D", " "}]], 
          RowBox[{"cosh", "(", "\[Beta]\[Mu]h", ")"}]}]}], " ", "]"}], 
       RowBox[{"N", "-", "1"}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "\[Beta]2"}], 
        SuperscriptBox["e", 
         RowBox[{"-", "\[Beta]D", " "}]], 
        RowBox[{"cosh", "(", "\[Beta]\[Mu]h", ")"}]}], ")"}]}], "=", " ", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        RowBox[{"2", 
         SuperscriptBox["\[Beta]NZe", 
          RowBox[{"-", "\[Beta]D", " "}]], 
         RowBox[{"cosh", "(", "\[Beta]\[Mu]h", ")"}]}], ")"}], 
       RowBox[{"[", 
        RowBox[{"1", " ", "+", " ", 
         RowBox[{"2", 
          SuperscriptBox["e", 
           RowBox[{"-", "\[Beta]D", " "}]], 
          RowBox[{"cosh", "(", "\[Beta]\[Mu]h", ")"}]}]}], " ", "]"}]]}]}]}], 
   TraditionalForm]],ExpressionUUID->"80aa6c2d-e976-4968-809f-bed5b5836577"],
 ", tenho, por fim, que o valor m\[EAcute]dio do momento de quadrupolo do \
sistema \[EAcute] dado por: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftAngleBracket]", "Q", "\[RightAngleBracket]"}], " ", "=", 
    "  ", 
    FractionBox[
     RowBox[{
      RowBox[{"2", 
       SuperscriptBox["Ne", 
        RowBox[{"-", "\[Beta]D", " "}]], 
       RowBox[{"cosh", "(", "\[Beta]\[Mu]h", ")"}]}], ")"}], 
     RowBox[{"[", 
      RowBox[{"1", " ", "+", " ", 
       RowBox[{"2", 
        SuperscriptBox["e", 
         RowBox[{"-", "\[Beta]D", " "}]], 
        RowBox[{"cosh", "(", "\[Beta]\[Mu]h", ")"}]}]}], " ", "]"}]]}], 
   TraditionalForm]],ExpressionUUID->"31d082ce-a349-4322-b0f5-d8f532b92f6a"]
}], "Text",
