       RowBox[{"\[CapitalOmega]", "(", "E", ")"}], " ", 
       SuperscriptBox["e", 
        RowBox[{"-", 
         SubscriptBox["\[Beta]E", "j"]}]]}]}], "=", " ", 
     RowBox[{
      RowBox[{"\[CapitalOmega]", "(", "E", ")"}], " ", 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["e", 
         RowBox[{"-", 
          FractionBox["\[Beta]\[Epsilon]", "2"]}]], " ", "+", " ", 
        SuperscriptBox["e", 
         FractionBox["\[Beta]\[Epsilon]", "2"]]}], ")"}]}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3ddb3944-e6b3-4017-a3bd-01b085782dd6"],
 ", onde as exponenciais podem ser simplificadas utilizando a rela\[CCedilla]\
\[ATilde]o de temperatura que encontramos n item anterior:\n\t\[FilledCircle]\
\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Beta]", "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       RowBox[{
        SubscriptBox["K", "B"], "T"}]], " ", "\[RightArrow]", 
      FractionBox["\[Beta]\[Epsilon]", "2"]}], "=", 
     RowBox[{
      FractionBox["\[Epsilon]", 
       RowBox[{"2", 
        SubscriptBox["K", "B"], "T"}]], " ", "=", " ", 
      RowBox[{
       RowBox[{
        FractionBox["1", "2"], "ln", 
        FractionBox[
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", 
             FractionBox["E", "\[Epsilon]"]}], " ", "+", " ", 
            FractionBox["N", "2"]}], ")"}], " "}], 
         RowBox[{" ", 
          RowBox[{"(", 
           RowBox[{
            FractionBox["E", "\[Epsilon]"], " ", "+", " ", 
            FractionBox["N", "2"]}], ")"}]}]]}], " ", "=", " ", 
       RowBox[{"ln", 
        SqrtBox[
         FractionBox[
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", 
              FractionBox["E", "\[Epsilon]"]}], " ", "+", " ", 
             FractionBox["N", "2"]}], ")"}], " "}], 
          RowBox[{" ", 
