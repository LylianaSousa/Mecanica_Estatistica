Cell[TextData[{
 "Para determinar a temperatura, utilizo a rela\[CCedilla]\[ATilde]o:\n ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", "T"], "=", 
    RowBox[{
     SubscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"\[PartialD]", "S"}], 
        RowBox[{"\[PartialD]", "E"}]], ")"}], 
      RowBox[{"N", ",", "V"}]], " ", "\[TildeTilde]", 
     RowBox[{
      RowBox[{"-", 
       SubscriptBox["K", "B"]}], 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", "E"}]], 
       RowBox[{"(", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            FractionBox["E", "\[Epsilon]"], " ", "+", " ", 
            FractionBox["N", "2"]}], ")"}], "  ", 
          RowBox[{"ln", "(", 
           RowBox[{
            FractionBox["E", "\[Epsilon]"], " ", "+", " ", 
            FractionBox["N", "2"]}], ")"}]}], "   ", "+", " ", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", 
             FractionBox["E", "\[Epsilon]"]}], " ", "+", " ", 
            FractionBox["N", "2"]}], ")"}], "  ", 
          RowBox[{"ln", "(", 
           RowBox[{
            RowBox[{"-", 
             FractionBox["E", "\[Epsilon]"]}], " ", "+", " ", 
            FractionBox["N", "2"]}], ")"}]}]}], "  ", ")"}]}]}], " ", 
     "\[TildeTilde]", 
     RowBox[{"-", 
      RowBox[{
       SubscriptBox["K", "B"], "(", "   ", 
       RowBox[{
        FractionBox[
         RowBox[{
          RowBox[{"ln", 
           RowBox[{"(", 
            RowBox[{
             FractionBox["E", "\[Epsilon]"], " ", "+", " ", 
             FractionBox["N", "2"]}], ")"}]}], " ", "+", "1"}], "\[Epsilon]"],
         "  ", "-", 
        FractionBox[
         RowBox[{"  ", 
          RowBox[{
           RowBox[{"ln", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"-", 
               FractionBox["E", "\[Epsilon]"]}], " ", "+", " ", 
              FractionBox["N", "2"]}], ")"}]}], " ", "+", "1"}]}], 
         "\[Epsilon]"]}], "  ", ")"}]}], " ", "\[TildeTilde]", 
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        SubscriptBox["K", "B"], "\[Epsilon]"]}], 
      RowBox[{"(", "   ", 
       RowBox[{
        RowBox[{"ln", "(", 
         RowBox[{
          FractionBox["E", "\[Epsilon]"], " ", "+", " ", 
          FractionBox["N", "2"]}], ")"}], "   ", "-", "  ", 
        RowBox[{"ln", "(", 
         RowBox[{
          RowBox[{"-", 
           FractionBox["E", "\[Epsilon]"]}], " ", "+", " ", 
          FractionBox["N", "2"]}], ")"}]}], ")"}]}], " ", "\[TildeTilde]", 
     RowBox[{
      FractionBox[
       SubscriptBox["K", "B"], "\[Epsilon]"], 
      RowBox[{"(", " ", 
       RowBox[{
        RowBox[{"-", "  ", 
         RowBox[{"ln", "(", 
          RowBox[{
           FractionBox["E", "\[Epsilon]"], " ", "+", " ", 
           FractionBox["N", "2"]}], ")"}]}], "   ", "+", "  ", 
        RowBox[{"ln", "(", 
         RowBox[{
          RowBox[{"-", 
           FractionBox["E", "\[Epsilon]"]}], " ", "+", " ", 
          FractionBox["N", "2"]}], ")"}]}], " ", ")"}]}], "  ", 
     "\[TildeTilde]", 
     RowBox[{
      FractionBox[
       SubscriptBox["K", "B"], "\[Epsilon]"], "ln", 
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", 
           FractionBox["E", "\[Epsilon]"]}], " ", "+", " ", 
          FractionBox["N", "2"]}], ")"}], " "}], 
       RowBox[{" ", 
        RowBox[{"(", 
         RowBox[{
          FractionBox["E", "\[Epsilon]"], " ", "+", " ", 
          FractionBox["N", "2"]}], ")"}]}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0193c3e1-4241-49e2-8793-805a5594367e"],
 "\n Com isso, chego na seguinte rela\[CCedilla]\[ATilde]o para a \
temperatura:\n ",
 Cell[BoxData[
  FormBox[
   RowBox[{"T", "\[TildeTilde]", 
    RowBox[{
     FractionBox["\[Epsilon]", 
      SubscriptBox["K", "B"]], 
     SuperscriptBox[
      RowBox[{"ln", "[", 
       FractionBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", 
            FractionBox["E", "\[Epsilon]"]}], " ", "+", " ", 
           FractionBox["N", "2"]}], ")"}], " "}], 
        RowBox[{" ", 
         RowBox[{"(", 
          RowBox[{
           FractionBox["E", "\[Epsilon]"], " ", "+", " ", 
           FractionBox["N", "2"]}], ")"}]}]], "]"}], 
      RowBox[{"-", "1"}]]}], "\[TildeTilde]", 
    RowBox[{
     FractionBox["\[Epsilon]", 
      SubscriptBox["K", "B"]], 
     SuperscriptBox[
      RowBox[{"ln", "[", 
       RowBox[{"1", " ", "-", " ", 
        FractionBox[
         RowBox[{"2", "E"}], 
         RowBox[{"E", " ", "+", " ", 
          FractionBox["N\[Epsilon]", "2"]}]]}], "]"}], 
      RowBox[{"-", "1"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7add5034-f278-4d5d-ab0b-d083992cce37"],
 ", para E >0, tenho que ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"1", " ", "-", " ", 
      FractionBox[
       RowBox[{"2", "E"}], 
       RowBox[{"E", " ", "+", " ", 
        FractionBox["N\[Epsilon]", "2"]}]]}], ">", "0"}], " ", 
    "\[RightArrow]", 
    RowBox[{"0", "<", " ", 
     FractionBox[
      RowBox[{"2", "E"}], 
      RowBox[{"E", " ", "+", " ", 
       FractionBox["N\[Epsilon]", "2"]}]], "<", "1"}]}], TraditionalForm]],
  ExpressionUUID->"746166fd-43d5-41a7-91fb-f7eada922640"],
 ", o que indica que o valor do logaritmo ser\[AAcute] sempre negativo, \
fazendo com que a nossa temperatura seja negativa."
}], "Text",
