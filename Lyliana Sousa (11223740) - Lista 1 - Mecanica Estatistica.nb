             RowBox[{"4", " ", 
              RowBox[{"(", 
               RowBox[{"3", "+", 
                RowBox[{"3", "p"}]}], ")"}]}]}], ")"}], " ", "+", " ", 
           RowBox[{"q", "(", 
            RowBox[{"9", " ", "+", " ", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"3", " ", "+", 
