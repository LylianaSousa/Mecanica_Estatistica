Cell[TextData[{
 "Para encontrarmos os valores de ",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", " ", "=", " ", 
    RowBox[{"(", GridBox[{
       {"a", "b"},
       {"b", "c"}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "47bbdf92-7e7b-4d3a-a0d8-6b02d8b0bafe"],
 ", usamos a rela\[CCedilla]\[ATilde]o ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"det", "(", 
     RowBox[{"H", " ", "-", 
      SubscriptBox["\[Lambda]I", "n"]}], ")"}], " ", "=", " ", "0"}], 
   TraditionalForm]],ExpressionUUID->"2b50203b-7add-467f-9b3e-74974c688686"],
 ". Assim, teremos que:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"det", "[", 
     RowBox[{GridBox[{
        {
         RowBox[{"a", "-", "\[Lambda]"}]},
        {"b"}
       }], GridBox[{
        {"b"},
        {
         RowBox[{"c", "-", "\[Lambda]"}]}
       }]}], "]"}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"0", " ", "\[LongRightArrow]", 
        RowBox[{"(", 
         RowBox[{"a", "-", "\[Lambda]"}], ")"}]}], 
       RowBox[{"(", 
        RowBox[{"c", "-", "\[Lambda]"}], ")"}]}], " ", "-", " ", 
      SuperscriptBox["b", 
       RowBox[{"2", " "}]]}], " ", "=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"0", " ", "\[LongRightArrow]", " ", "a"}], " ", "c"}], " ", 
       "-", " ", 
       RowBox[{"a", " ", "\[Lambda]"}], " ", "-", " ", 
       RowBox[{"c", " ", "\[Lambda]"}], " ", "+", 
       SuperscriptBox[
        RowBox[{"\[Lambda]", " "}], "2"], "  ", "-", " ", 
       SuperscriptBox["b", 
        RowBox[{"2", " "}]]}], " ", "=", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"0", " ", "\[LongRightArrow]", "   ", 
         SuperscriptBox[
          RowBox[{"\[Lambda]", " "}], "2"]}], "  ", "-", " ", 
        RowBox[{
         RowBox[{"(", " ", 
          RowBox[{"a", " ", "+", " ", "c"}], " ", ")"}], "\[Lambda]"}], " ", 
        "-", " ", 
        SuperscriptBox[
         RowBox[{"b", " "}], 
         RowBox[{"2", " "}]], " ", "+", " ", 
        RowBox[{"a", " ", "c"}]}], "=", " ", "0", " "}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"c6e0080d-a225-4872-8905-73af1f4a9983"],
 "\nDisso teremos que, ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     SubscriptBox["\[Lambda]", "\[PlusMinus]"], " ", "=", " ", 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"a", " ", "+", " ", "c"}], ")"}], " ", "\[PlusMinus]", " ", 
       SqrtBox[
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"a", " ", "+", " ", "c"}], ")"}], "2"], " ", "+", " ", 
         RowBox[{"4", 
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["b", 
             RowBox[{"2", " "}]], " ", "-", 
            RowBox[{"a", " ", "c"}]}], ")"}]}]}]]}], "2"]}]}], 
   TraditionalForm]],ExpressionUUID->"5a48069f-30d2-4c43-b8e4-3ee24a1b757c"],
 ". Assim, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]\[Lambda]", " ", "=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Lambda]", "+"], " ", "-", " ", 
      SubscriptBox["\[Lambda]", "-"]}], " ", "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"a", " ", "+", " ", "c"}], ")"}], "  ", "+", " ", 
        SqrtBox[
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"a", " ", "+", " ", "c"}], ")"}], "2"], " ", "+", " ", 
          RowBox[{"4", 
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["b", 
              RowBox[{"2", " "}]], " ", "-", 
             RowBox[{"a", " ", "c"}]}], ")"}]}]}]], " ", "-", " ", 
        RowBox[{"(", 
         RowBox[{"a", " ", "+", " ", "c"}], ")"}], " ", "+", " ", 
        SqrtBox[
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"a", " ", "+", " ", "c"}], ")"}], "2"], " ", "+", " ", 
          RowBox[{"4", 
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["b", 
              RowBox[{"2", " "}]], " ", "-", 
             RowBox[{"a", " ", "c"}]}], ")"}]}]}]]}], "2"], "=", 
      RowBox[{
       SqrtBox[
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"a", " ", "+", " ", "c"}], ")"}], "2"], " ", "+", " ", 
         RowBox[{"4", 
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["b", 
             RowBox[{"2", " "}]], " ", "-", 
            RowBox[{"a", " ", "c"}]}], ")"}]}]}]], "=", 
       RowBox[{
        SqrtBox[
         RowBox[{
          SuperscriptBox[
           RowBox[{"a", " "}], 
           RowBox[{"2", " "}]], "+", 
          RowBox[{"2", " ", "a", " ", "c"}], " ", "+", " ", 
          SuperscriptBox["c", "2"], " ", "+", " ", 
          RowBox[{"4", " ", 
           SuperscriptBox["b", "2"]}], " ", "-", " ", 
          RowBox[{"4", " ", "a", " ", "c"}]}]], "=", " ", 
        RowBox[{
         SqrtBox[
          RowBox[{
           SuperscriptBox[
            RowBox[{"a", " "}], 
            RowBox[{"2", " "}]], "-", 
           RowBox[{"2", " ", "a", " ", "c"}], " ", "+", " ", 
           SuperscriptBox["c", "2"], " ", "+", " ", 
           RowBox[{"4", " ", 
            SuperscriptBox["b", "2"], " "}]}]], " ", "=", " ", 
         SqrtBox[
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"a", " ", "-", " ", "c"}], ")"}], "2"], " ", "+", " ", 
           RowBox[{"4", 
            SuperscriptBox["b", 
             RowBox[{"2", " "}]], " "}]}]]}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"b3be2271-f4ea-4656-912f-75142cf3ac6a"]
}], "Text",
