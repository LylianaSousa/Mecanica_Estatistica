        RowBox[{
         RowBox[{
          UnderoverscriptBox["\[Sum]", 
           RowBox[{"i", "=", "1"}], "6"], 
          RowBox[{
           SubscriptBox["p", "i"], "[", 
           RowBox[{
            SuperscriptBox[
             SubscriptBox["x", "i"], "2"], "+", " ", 
            SuperscriptBox[
             OverscriptBox["x", "-"], "2"], " ", "-", 
            RowBox[{"2", " ", 
             SubscriptBox[
              RowBox[{"x", " "}], "i"], 
             OverscriptBox["x", "-"]}]}], " ", "]"}]}], "  ", "=", 
         RowBox[{
          RowBox[{
           RowBox[{"q", "(", 
            RowBox[{"1", " ", "+", " ", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"3", " ", "+", 
