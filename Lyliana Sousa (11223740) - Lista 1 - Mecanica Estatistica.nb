          RowBox[{"dc", " ", 
           RowBox[{"\[Delta]", "(", 
            RowBox[{"\[CapitalDelta]\[Lambda]", " ", "-", " ", 
             SqrtBox[
              RowBox[{
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{"c", " ", "-", " ", "a"}], " ", ")"}], "2"], " ", 
               "+", 
               RowBox[{"4", 
                SuperscriptBox["b", "2"]}]}]]}], ")"}], 
