              RowBox[{"3", "/", "2"}]]}]]}]}]}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"f2441591-7b75-4224-87e3-48e4182863f0"],
 "\nRealizando a mudan\[CCedilla]a de vari\[AAcute]vel: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", " ", "=", " ", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"c", " ", "+", " ", "a"}], "2"], " ", "e", " ", "y"}], " ", 
     "=", " ", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"c", "-", "a"}], "2"], " ", "\[LongRightArrow]", " ", "a"}], 
      "=", 
      RowBox[{
       RowBox[{"x", "-", 
        RowBox[{"y", " ", "e", " ", "c"}]}], "=", " ", 
       RowBox[{"x", "+", "y"}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "8d9cebab-95fa-4605-a26f-0c2c921eb5de"],
 ". Terei que:\n\[FilledSquare]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SqrtBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"c", " ", "-", " ", "a"}], " ", ")"}], "2"], " ", "+", 
      RowBox[{"4", 
       SuperscriptBox["b", "2"]}]}]], " ", "=", " ", 
    RowBox[{
     SqrtBox[
      RowBox[{
       RowBox[{"4", " ", 
        SuperscriptBox["y", "2"]}], " ", "+", " ", 
       RowBox[{"4", 
        SuperscriptBox["b", "2"]}]}]], " ", "=", " ", 
     RowBox[{"2", 
      SqrtBox[
       RowBox[{
        SuperscriptBox["y", "2"], " ", "+", " ", 
        SuperscriptBox["b", "2"]}]]}]}]}], TraditionalForm]],ExpressionUUID->
  "ed2bbcf7-7be8-45f0-a591-d4ecb76d3adf"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[FilledSquare]", "\t", 
      SuperscriptBox["a", "2"]}], " ", "+", 
     SuperscriptBox["b", "2"], "+", 
     SuperscriptBox["c", "2"]}], " ", "=", " ", 
    RowBox[{
     RowBox[{"2", 
      SuperscriptBox["x", 
       RowBox[{"2", " "}]]}], " ", "+", " ", 
