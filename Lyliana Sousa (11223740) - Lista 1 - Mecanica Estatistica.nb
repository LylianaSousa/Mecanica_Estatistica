             RowBox[{"2", " ", 
              RowBox[{"(", 
               RowBox[{"3", "+", 
                RowBox[{"3", "p"}]}], ")"}]}]}], ")"}], " ", "+", " ", 
           RowBox[{"q", "(", 
            RowBox[{"4", "+", " ", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"3", " ", "+", 
