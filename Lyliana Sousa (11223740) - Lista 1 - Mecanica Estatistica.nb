        RowBox[{"db", 
         RowBox[{
          SubsuperscriptBox["\[Integral]", 
           RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
          RowBox[{"dc", " ", 
           RowBox[{"\[Delta]", "(", 
            RowBox[{"\[CapitalDelta]\[Lambda]", " ", "-", " ", 
             SqrtBox[
              RowBox[{
               SuperscriptBox[
                RowBox[{"(", 
